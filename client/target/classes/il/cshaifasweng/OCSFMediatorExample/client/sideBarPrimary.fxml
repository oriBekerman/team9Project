<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="sideBar" prefHeight="871.0" prefWidth="226.0" styleClass="sidebar" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.SideBarPrimaryBoundary">
   <children>
      <ImageView fx:id="MOMSImage" fitHeight="228.0" fitWidth="226.0" pickOnBounds="true" preserveRatio="true" styleClass="sidebar .image-container" />
      <VBox prefHeight="329.0" prefWidth="190.0" spacing="10.0" styleClass="sidebar">
         <children>
            <Button fx:id="menuBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#navToMenu" prefHeight="49.0" prefWidth="190.0" text="Menu" />
            <Button fx:id="toggleButtonBranch" layoutX="25.0" layoutY="239.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="190.0" text="Our Branches" />
            <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#navToLogin" prefHeight="49.0" prefWidth="190.0" styleClass="menu-btn" text="Login" />
            <Button fx:id="logoutBtn" mnemonicParsing="false" onAction="#navToLogOut" prefHeight="49.0" prefWidth="190.0" styleClass="menu-btn" text="Logout" />
            <Button fx:id="updateMenuBtn" layoutX="10.0" layoutY="187.0" mnemonicParsing="false" onAction="#navToUpdateMenu" prefHeight="49.0" prefWidth="190.0" styleClass="menu-btn" text="Update Menu" />
            <Button fx:id="toggleButtonReports" layoutX="10.0" layoutY="246.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="190.0" styleClass="menu-btn" text="Reports" />
         </children>
      </VBox>
      <VBox prefHeight="43.0" prefWidth="202.0" />
   </children>
</VBox>
