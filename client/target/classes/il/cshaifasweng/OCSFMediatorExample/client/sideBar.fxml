<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="sideBar" prefHeight="871.0" prefWidth="226.0" styleClass="sidebar" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.SideBarBoundary">
   <children>
      <ImageView fx:id="MOMSImage" fitHeight="228.0" fitWidth="226.0" pickOnBounds="true" preserveRatio="true" />
      <VBox prefHeight="329.0" prefWidth="190.0" spacing="10.0" styleClass="sidebar">
         <children>
            <Button fx:id="menuBtn" mnemonicParsing="false" onAction="#navToMenu" prefHeight="49.0" prefWidth="190.0" text="Menu" />
            <Button fx:id="toggleButtonBranch" layoutX="25.0" layoutY="239.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="190.0" text="Our Branches" />
            <Button fx:id="deliveryBtn" mnemonicParsing="false" onAction="#navToDeliveryPage" prefHeight="49.0" prefWidth="185.0" styleClass="menu-btn" text="Make a Delivery">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button fx:id="reservationBtn" mnemonicParsing="false" onAction="#navToReservationPage" prefHeight="49.0" prefWidth="181.0" styleClass="menu-btn" text="Make a Reservation">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button fx:id="complaintBtn" mnemonicParsing="false" onAction="#navToComplaintPage" prefHeight="49.0" prefWidth="185.0" styleClass="menu-btn" text="Make a Complaint">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <VBox prefHeight="43.0" prefWidth="202.0" />
   </children>
</VBox>
