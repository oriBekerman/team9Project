<AnchorPane prefHeight="338.0" prefWidth="524.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.LoginBoundary">
   <children>
      <Label layoutX="107.0" layoutY="94.0" text="User Name :">
