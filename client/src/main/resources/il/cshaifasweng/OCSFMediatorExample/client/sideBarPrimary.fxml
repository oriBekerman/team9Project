<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="sideBar" prefHeight="871.0" prefWidth="226.0" styleClass="sidebar" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.SideBarPrimaryBoundary">
    <children>
        <ImageView fx:id="MOMSImage" fitHeight="228.0" fitWidth="226.0" pickOnBounds="true" preserveRatio="true" styleClass="sidebar .image-container" />
        <VBox prefHeight="595.0" prefWidth="226.0" spacing="10.0" styleClass="sidebar">
            <children>
                <Button fx:id="menuBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#navToMenu" prefHeight="49.0" prefWidth="216.0" text="Menu" />
                <Button fx:id="toggleButtonBranch" layoutX="25.0" layoutY="239.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="216.0" text="Our Branches" />
                <Button fx:id="reservationBtn" layoutX="10.0" layoutY="69.0" mnemonicParsing="false" onAction="#navToReservation" prefHeight="49.0" prefWidth="216.0" text="Make a Redervation">
               <font>
                  <Font size="9.0" />
               </font></Button>
                <Button fx:id="subCompBtn" layoutX="10.0" layoutY="292.0" mnemonicParsing="false" onAction="#goToSubCompPage" prefHeight="49.0" prefWidth="216.0" text="Submit a Complaint">
               <font>
                  <Font size="9.0" />
               </font></Button>
                <Button fx:id="cancelDelivery" layoutX="10.0" layoutY="364.0" mnemonicParsing="false" onAction="#cancelDel" prefHeight="49.0" prefWidth="216.0" text="Cancel Delivery" />
                <Button fx:id="updateMenuBtn" layoutX="10.0" layoutY="187.0" mnemonicParsing="false" onAction="#navToMenu" prefHeight="49.0" prefWidth="216.0" text="Update Menu" />
                <Button fx:id="complaintsTableBtn" layoutX="10.0" layoutY="423.0" mnemonicParsing="false" onAction="#viewComplaints" prefHeight="49.0" prefWidth="216.0" text="View Complaints">
               <font>
                  <Font size="10.0" />
               </font></Button>
                <Button fx:id="givePermitBtn" layoutX="10.0" layoutY="423.0" mnemonicParsing="false" onAction="#givePermit" prefHeight="49.0" prefWidth="216.0" text="Give Premitions" />
                <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#navToLogin" prefHeight="49.0" prefWidth="216.0" text="Login" />
                <Button fx:id="logoutBtn" mnemonicParsing="false" onAction="#navToLogOut" prefHeight="49.0" prefWidth="216.0" text="Logout" />
            </children>
        </VBox>
        <VBox prefHeight="43.0" prefWidth="202.0" />
    </children>
</VBox>
