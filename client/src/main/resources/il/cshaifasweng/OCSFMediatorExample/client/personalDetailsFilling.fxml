<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="335.0" prefWidth="523.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.PersonalDetailsFillingBoundary">
   <children>
      <Label fx:id="nameLabel" layoutX="68.0" layoutY="68.0" prefHeight="40.0" prefWidth="91.0" styleClass="subtitle" text="Full name :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="phoneLabel" layoutX="68.0" layoutY="112.0" styleClass="subtitle" text="Phone number :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="mailLabel" layoutX="68.0" layoutY="152.0" styleClass="subtitle" text="Mail address :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" layoutX="205.0" layoutY="76.0" />
      <TextField fx:id="phoneTextField" layoutX="205.0" layoutY="120.0" />
      <TextField fx:id="mailTextField" layoutX="205.0" layoutY="160.0" />
      <Button fx:id="contToCCInfoBtn" layoutX="405.0" layoutY="249.0" mnemonicParsing="false" onAction="#contToCCinfoFill" styleClass="return-btn" text="Continue" />
      <Button fx:id="backBtn" layoutX="34.0" layoutY="249.0" mnemonicParsing="false" onAction="#backToReservation" styleClass="return-btn" text="Back" />
      <Label fx:id="errorLabel" layoutX="65.0" layoutY="212.0" prefHeight="17.0" prefWidth="329.0" />
   </children>
</AnchorPane>
